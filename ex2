print("Варианты покупки на 100 рублей:")
variant_count = 0
for bulls in range(0, 11): #Считаем быков (по 10 каждый) от 0 до 10
    for cows in range(0, 21): #Считаем коров (по 5 каждая) от 0 до 20
        for calves in range(0, 201): #Считаем телят (по 0.5 каждый) от 0 до 200
            #Проверяем хватает ли денег
            if bulls * 10 + cows * 5 + calves * 0.5 == 100:
                variant_count += 1
                print(f"Вариант {variant_count} быков: {bulls}, коров: {cows}, телят: {calves}")
